<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">
  </head>
  <body>
    <h1><%= title %></h1>
   
    <a class="btb btn-success" href="/slots/add">Add Slot</a>
    <div class="row">
      <%slotList.forEach(function(slot){%>
      <div class="card" style="width: 20rem; margin: 8px;">
      <div class="card-body">
      <h5 class="card-title">DoctorId: <%=slot.doctorId%></h5>
  
      <br>
      <p class="card-text"><%=slot.date%>
      </p>
      <h6 class="card-subtitle mb-2 text-muted">startTime: <%=slot.startTime%></h6>
      <h6 class="card-subtitle mb-2 text-muted">endTime: <%=slot.endTime%></h6>
      <h6 class="card-subtitle mb-2 text-muted">Status: <%=slot.status%></h6>
      <a href="/slots/edit/<%=slot._id%>" class="card-link">Edit</a>
      <a href="/slots/delete/<%=slot._id%>" class="card-link" action="">Delete</a>
      </div>
      </div>
      <%})%>
      </div>
      
  </body>
</html>